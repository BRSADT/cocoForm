    <class>cocoformpruebas.Encuesta</class>
    <class>cocoformpruebas.Escala</class>
    <class>cocoformpruebas.Pregunta</class>
    <class>cocoformpruebas.Opcmultiple</class>
    <class>cocoformpruebas.Respuesta</class>
    <class>cocoformpruebas.Categoria</class>
    <class>cocoformpruebas.Usuario</class>
    <class>cocoformpruebas.TRespuestas</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/coco1?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
